<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="app.dice.values.6.numbers" type="collection">
            <parameter>1</parameter>
            <parameter>2</parameter>
            <parameter>3</parameter>
            <parameter>4</parameter>
            <parameter>5</parameter>
            <parameter>6</parameter>
        </parameter>
    </parameters>

    <services>
        <service id="app.dice.six" class="AppBundle\Service\SixEyedDice" />
        <service id="app.dice_controller" class="AppBundle\Controller\DiceController">
            <argument type="service" id="app.dice.six"/>
        </service>
    </services>

</container>